#map.well{data: {locations: @locations.to_json, center: params[:search]}}

%table.table.table-bordered.table-striped
  %thead
    %tr
      %th Name
      %th Address
      %th Website

  %tbody
    - @locations.each do |location|
      %tr
        %td= location.name
        %td= location.address
        %td= location.website
